<!DOCTYPE project>
<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:l="sap.ui.layout"
   xmlns:core="sap.ui.core" >
   <Dialog id="dlgTemplate" title="{i18n>cptTemplate}">
      <l:VerticalLayout id="_IDGenVerticalLayout1t" class="sapUiContentPadding" width="100%">
        <l:content>
          <Label id="_IDGenLabel1tpl" text="{i18n>lblOrderNum}" labelFor="fldDocument"/>
          <Input id="fldDocumentpl" class="sapUiSmallMarginBottom" maxLength="30" change="onTemplateDialogSend"/>
        </l:content>
        <l:content>
          <Label id="_IDGenLabel2tpl" text="{i18n>txtPosNr}" labelFor="lblPosition"/>
          <Input id="fldPositiontpl" class="sapUiSmallMarginBottom" maxLength="30" change="onTemplateDialogSend"/>
        </l:content>
        <l:content>

                        <Table id="listTemplates" class="sapUiResponsiveMargin" width="auto">															
							  <columns>
								<Column id="_IDGenColumn1tpl" width="20%">					
								<Text id="_IDGenText1ttpl" />				
								</Column>								
								<Column id="_IDGenColumn2tpl">
								<Text id="_IDGenText2ttpl" />
								</Column>
								<Column id="_IDGenColumn3tpl">
								<Text id="_IDGenText3ttpl" />
								</Column>
							  </columns>
							<items>
							</items>
						  </Table>

        </l:content>
      </l:VerticalLayout>

      <buttons>
         <Button id="btnTemplateLoad" type="Accept" text="{i18n>btnLoad}" />
<!--          <Button id="btnProfileSave" text="{i18n>btnSave}"  press="onProfileSave"/> -->
         <Button id="btnTemplateCancel" type="Reject" text="{i18n>btnClose}"  press="onTemplateCancel"/>
      </buttons>

   </Dialog>
</core:FragmentDefinition>